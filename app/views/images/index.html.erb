<p id="notice"><%= notice %></p>

<h1>Images</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Subset</th>
      <th>Original url</th>
      <th>Original landing url</th>
      <th>License</th>
      <th>Author profile url</th>
      <th>Author</th>
      <th>Title</th>
      <th>Original size</th>
      <th>Original md5</th>
      <th>Thumbnail 300k url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.image_id %></td>
        <td><%= image.subset %></td>
        <td><%= image.original_url %></td>
        <td><%= image.original_landing_url %></td>
        <td><%= image.license %></td>
        <td><%= image.author_profile_url %></td>
        <td><%= image.author %></td>
        <td><%= image.title %></td>
        <td><%= image.original_size %></td>
        <td><%= image.original_md5 %></td>
        <td><%= image.thumbnail_300k_url %></td>
        <td><%= link_to 'Show', image %></td>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image', new_image_path %>
